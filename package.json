{
  "name": "rentx",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --silent=false",
    "test:functional": "NODE_ENV=test jest --projects ./tests --forceExit --runInBand",
    "test:watch": "NODE_ENV=test jest --silent=false --w",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "dev": "NODE_ENV=development tsnd -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/server",
    "start": "node ./dist/server.js",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "seed:admin": "tsnd --ignore-watch node_modules src/shared/infra/typeorm/seed/admin.ts",
    "typeorm": "NODE_ENV=development ts-node -r tsconfig-paths/register --transpile-only ./node_modules/typeorm/cli",
    "up": "npm run build && sudo docker-compose up -d",
    "down": "sudo docker-compose down",
    "rebuild:app": "sudo docker-compose build && sudo docker-compose rm --stop -f && docker-compose up --force-recreate"
  },
  "keywords": [
    "SOLID",
    "Typescript",
    "Tests",
    "Swagger"
  ],
  "author": "Matheus Santos Araujo da Silva <souzamatheus8174@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "better-sqlite3": "^7.6.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "git-commit-msg-linter": "^4.1.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "csv-parser": "^3.0.0",
    "dayjs": "^1.11.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.7",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.2.4",
    "swagger-ui-express": "^4.3.0",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.2.45"
  },
  "engines": {
    "node": "18.5.0"
  }
}
